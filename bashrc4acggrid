    eval "$__conda_setup"
else
    if [ -f "/mnt/castor/seas_home/a/athakore/mambaforge/etc/profile.d/conda.sh" ]; then
        . "/mnt/castor/seas_home/a/athakore/mambaforge/etc/profile.d/conda.sh"
    else
        export PATH="/mnt/castor/seas_home/a/athakore/mambaforge/bin:$PATH"
    fi
fi
